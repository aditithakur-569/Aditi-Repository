    "    idx = 0\n",
    "    # index to travese the array\n",
    "    arr_idx = k+1\n",
    "    for i in range(0,k+1):\n",
    "        heap.append(arr[i])\n",
    "  \n",
    "    n = len(heap)\n",
    "    Min_Heapify(heap,n,0)\n",
    "    while arr_idx < len(arr):\n",
    "        \n",
