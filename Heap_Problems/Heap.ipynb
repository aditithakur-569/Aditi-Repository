    "##K-th Largest Sum Contiguous Subarray.[start-end:May 7th,2021]\n",
    "import heapq\n",
    "\n",
    "def kthLargestSum(arr, n, k):\n",
    "     \n",
    "    total = []\n",
    "    heap = []\n",
    "    total.append(0)\n",
    "    total.append(arr[0])\n",
    "    for i in range(2, n + 1):\n",
    "        total.append(total[i - 1] + arr[i - 1])\n",
    "   \n",
    "    for i in range(1,n + 1):\n",
    "        \n",
    "        for j in range(i, n + 1):\n",
    "            \n",
    "            x = total[j] - total[i - 1]; \n",
    "            if len(heap) < k:\n",
    "                heapq.heappush(heap,x)\n",
    "                heapq._heapify_max(heap)\n",
    "            else:\n",
